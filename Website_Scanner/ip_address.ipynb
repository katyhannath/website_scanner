    "#!/usr/bin/env python3\n",
    "import os\n",
    "\n",
    "def get_ip_address(url):\n",
    "    command = 'host ' + url\n",
    "    process = os.popen(command)\n",
    "    results = str(process.read())\n",
    "    marker = results.find('has address') + 12\n",
    "    return results[marker:].splitlines()\n",
    "\n",
